<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>authFilter</filter-name>
    <filter-class>filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>authFilter</filter-name>
    <url-pattern>/panel</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>signupservlet</servlet-name>
    <servlet-class>controller.SignupServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>panelServlet</servlet-name>
    <servlet-class>controller.PanelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BuyTicketServlet</servlet-name>
    <servlet-class>controller.BuyTicketServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteTicketServlet</servlet-name>
    <servlet-class>controller.DeleteTicketServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TravelHistory</servlet-name>
    <servlet-class>controller.TravelHistoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signupservlet</servlet-name>
    <url-pattern>/signupservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>panelServlet</servlet-name>
    <url-pattern>/panel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BuyTicketServlet</servlet-name>
    <url-pattern>/BuyTicketServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteTicketServlet</servlet-name>
    <url-pattern>/DeleteTicketServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TravelHistory</servlet-name>
    <url-pattern>/TravelHistory</url-pattern>
  </servlet-mapping>
</web-app>
